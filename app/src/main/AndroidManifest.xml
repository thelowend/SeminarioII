<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.prototipo.seminarioii.celiacos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.google.prototipo.seminarioii.celiacos.ChooserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.CustomAuthActivity"
            android:label="@string/label_custom_auth" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.GoogleSignInActivity"
            android:label="@string/label_google_sign_in" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.FacebookLoginActivity"
            android:label="@string/label_facebook_login" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.EmailPasswordActivity"
            android:label="@string/label_emailpassword"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.PhoneAuthActivity"
            android:label="@string/label_phone_auth"></activity>
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.AnonymousAuthActivity"
            android:label="@string/label_anonymous_auth" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.TwitterLoginActivity"
            android:label="@string/label_twitter_login" />
        <activity
            android:name="com.google.prototipo.seminarioii.celiacos.FirebaseUIActivity"
            android:label="@string/label_firebase_ui" />
    </application>

</manifest>
